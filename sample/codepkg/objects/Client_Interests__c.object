    <deploymentStatus>Deployed</deploymentStatus>
    <description>Master-detail object for tracking Client interests and the reasons for the interest.</description>
    <enableActivities>true</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <fields>
        <fullName>Client__c</fullName>
        <description>Master detail relationship field that relates Client Interests to Clients (Account)</description>
        <externalId>false</externalId>
        <label>Client</label>
        <referenceTo>Account</referenceTo>
        <relationshipName>Client_Interests</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Interest_Type__c</fullName>
        <description>Is the client interested in buying or selling this security?</description>
        <externalId>false</externalId>
        <label>Interest Type</label>
        <picklist>
            <picklistValues>
                <fullName>Buy</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Sell</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>On_Watch_List__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Indicates whether the Client Interest has been added to the Watch List or not.</description>
        <externalId>false</externalId>
        <label>On Watch List</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Reason_for_Interest__c</fullName>
        <description>Why is the Client interested?</description>
        <externalId>false</externalId>
        <label>Reason for Interest</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <label>Client Interest</label>
    <listViews>
        <fullName>AllClientInterests</fullName>
        <columns>NAME</columns>
        <columns>Client__c</columns>
        <columns>Interest_Type__c</columns>
        <columns>Reason_for_Interest__c</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Everything</filterScope>
        <label>All Client Interests</label>
    </listViews>
    <listViews>
        <fullName>MyClientInterestsAll</fullName>
        <columns>NAME</columns>
        <columns>Client__c</columns>
        <columns>Interest_Type__c</columns>
        <columns>Reason_for_Interest__c</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Mine</filterScope>
        <label>My Client Interests - All</label>
    </listViews>
    <listViews>
        <fullName>MyClientInterestsBuy</fullName>
        <columns>NAME</columns>
        <columns>Client__c</columns>
        <columns>Reason_for_Interest__c</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Mine</filterScope>
        <filters>
            <field>Interest_Type__c</field>
            <operation>equals</operation>
            <value>Buy</value>
        </filters>
        <label>My Client Interests - Buy</label>
    </listViews>
    <listViews>
        <fullName>MyClientInterestsSell</fullName>
        <columns>NAME</columns>
        <columns>Client__c</columns>
        <columns>Reason_for_Interest__c</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Mine</filterScope>
        <filters>
            <field>Interest_Type__c</field>
            <operation>equals</operation>
            <value>Sell</value>
        </filters>
        <label>My Client Interests - Sell</label>
    </listViews>
    <nameField>
        <label>Symbol/Interest</label>
        <trackHistory>false</trackHistory>
        <type>Text</type>
    </nameField>
    <pluralLabel>Client Interests</pluralLabel>
